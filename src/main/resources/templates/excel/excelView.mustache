{{>layout/header}}

<div class="container-fluid my-3">
    <div class="row align-items-center">
        <div class="col col-3">
            <h2>Excel</h2>
        </div>
        <div class="col col-9">
            <button type="button" class="btn btn-primary" id="btn-excel-upload">Upload</button>
            <button type="button" class="btn btn-danger" id="btn-excel-download">Download</button>
        </div>
    </div>
</div>
<hr>

<div class="container-fluid">
    <div class="col-md-4">
        <form id="form" enctype="multipart/form-data">
            <div class="form-group">
                <label for="tableName">TableName</label>
                <input type="text" class="form-control" id="tableName" name="tableName" readonly>
            </div>
            <div class="form-group">
                <label for="title">Title</label>
                <input type="text" class="form-control" id="title" name="title" value="">
            </div>
            <div class="form-group">
                <label for="file">File</label>
                <input type="file" class="form-control" id="file" name="file" accept=".xlsx, .xls">

            </div>
            <div class="form-group">
                <label for="file2">File</label>
                <input type="file" class="form-control" id="file2" name="file2">
            </div>
        </form>

    </div>
</div>

<script src="/js/excel/excelView.js"></script>

{{>layout/footer}}